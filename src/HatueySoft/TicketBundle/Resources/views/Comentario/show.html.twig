{% extends '@HatueySoftTicket/layout.html.twig' %}

{% block title 'Comentario show' %}

{% block tituloPagina %}
    <h1>Comentario show</h1>
{% endblock %}

{% block content -%}

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Comentario</th>
                <td>{{ entity.Comentario }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('comentario') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('comentario_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
